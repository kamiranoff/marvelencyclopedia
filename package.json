    "del": "~1.2.0",
    "envify": "~3.4.0",
    "express-debug": "^1.1.1",
    "foldero": "~0.1.1",
    "glob": "~5.0.10",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.2.1",
    "gulp-data": "~1.2.0",
    "gulp-eslint": "~0.14.0",
    "gulp-htmlmin": "~1.1.3",
    "gulp-if": "~1.2.5",
    "gulp-imagemin": "~2.2.1",
    "gulp-jade": "~1.0.1",
    "gulp-load-plugins": "~0.10.0",
    "gulp-newer": "~0.5.1",
    "gulp-nodemon": "^2.0.4",
