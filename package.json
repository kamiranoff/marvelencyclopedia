{
  "name": "marvel-encyclopedia",
  "version": "0.0.1",
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "dependencies": {
    "jquery": "~2.1.3",
    "normalize.css": "~3.0.1",
    "backbone": "^1.2.3",
    "handlebars": "*",
    "knacss": "^4.3.3",
    "lodash": "^3.2.0",
    "body-parser": "*",
    "errorhandler": "^1.3.5",
    "express": "*",
    "jade": "^1.9.2",
    "log4js": "*",
    "mongodb": "*",
    "morgan": "*",
    "serve-favicon": "*",
    "socket.io": "*",
    "util": "*",
    "validator": " *",
    "wiredep": "*",
    "mongoose": "*"
