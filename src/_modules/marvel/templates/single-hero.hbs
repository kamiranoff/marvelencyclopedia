<h1 class="hero-name">{{character.name}}</h1>
<section class="hero-description">
<section class="hero-description-wiki">
  <div class="main-hero-image"><img src="{{character.thumbnail.path}}.{{character.thumbnail.extension}}"></div>
  <div class="hero-description-wiki-text">
    <p><strong>Real Name: </strong>{{character.wiki.real_name}}</p>
    <p><strong>Relatives: </strong>{{character.wiki.relatives}}</p>
    <p><strong>Eyes: </strong>{{character.wiki.eyes}}</p>
    <p><strong>Hair: </strong>{{character.wiki.hair}}</p>
    <p><strong>Height: </strong>{{character.wiki.height}}</p>
    <p><strong>Weight: </strong>{{character.wiki.weight}}</p>

    <hr class="divider">
    <p><strong>Aliases: </strong>{{character.wiki.aliases}}</p>
    <p><strong>Abilities: </strong>{{character.wiki.abilities}}</p>
     <p><strong>Education: </strong>{{character.wiki.education}}</p>
  </div>
</section>
  <h1>{{character.description}}</h1>

<section class="layout-single-column">
  <p><strong>DESC</strong>:{{character.wiki.bio}}</p>

</section>

</section>
